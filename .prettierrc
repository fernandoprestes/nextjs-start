{
  "tabWidth": 2,
  "semi": false,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "jsxSingleQuote": true,
  "quoteProps": "consistent",
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "singleAttributePerLine": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false
}
